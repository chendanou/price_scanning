{
  "name": "price-survey-app",
  "version": "1.0.0",
  "description": "Automated price survey application using AI-powered web scraping",
  "main": "dist/server/index.js",
  "scripts": {
    "dev": "nodemon --exec tsx src/server/index.ts",
    "build": "tsc -p tsconfig.server.json && tsc -p tsconfig.client.json && vite build --config src/client/vite.config.ts",
    "build:server": "tsc -p tsconfig.server.json",
    "build:client": "vite build --config src/client/vite.config.ts",
    "start": "node dist/server/index.js",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix"
  },
  "keywords": [
    "price-survey",
    "web-scraping",
    "playwright",
    "azure",
    "ai"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "compression": "^1.7.4",
    "playwright": "^1.40.0",
    "csv-parse": "^5.5.3",
    "csv-stringify": "^6.4.5",
    "multer": "^1.4.5-lts.1",
    "@azure/search-documents": "^12.0.0",
    "@azure/identity": "^4.0.0",
    "@azure/openai": "^1.0.0-beta.9",
    "winston": "^3.11.0",
    "dotenv": "^16.3.1",
    "zod": "^3.22.4",
    "express-validator": "^7.0.1",
    "express-rate-limit": "^7.1.5",
    "socket.io": "^4.6.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.5",
    "@types/express": "^4.17.21",
    "@types/multer": "^1.4.11",
    "@types/cors": "^2.8.17",
    "@types/compression": "^1.7.5",
    "typescript": "^5.3.3",
    "ts-node": "^10.9.2",
    "tsx": "^4.7.0",
    "nodemon": "^3.0.2",
    "vitest": "^1.0.4",
    "@vitest/ui": "^1.0.4",
    "vite": "^5.0.8",
    "eslint": "^8.56.0",
    "@typescript-eslint/parser": "^6.15.0",
    "@typescript-eslint/eslint-plugin": "^6.15.0"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  }
}
